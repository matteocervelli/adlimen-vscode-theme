{
  "name": "Adlimen Theme Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#1B2631",
    "editor.foreground": "#F5F5F5",
    "activityBar.background": "#233240",
    "activityBar.foreground": "#F5F5F5",
    "sideBar.background": "#1D2A36",
    "sideBar.foreground": "#DCE4ED",
    "editorGroupHeader.tabsBackground": "#233240",
    "tab.inactiveBackground": "#2C3E50",
    "tab.activeBackground": "#34495E",
    "tab.activeForeground": "#F5F5F5",
    "statusBar.background": "#008080",
    "statusBar.foreground": "#F5F5F5",
    "titleBar.activeBackground": "#233240",
    "titleBar.activeForeground": "#F5F5F5",
    
    "editor.lineHighlightBackground": "#2C3E5066",
    "editor.selectionBackground": "#34495E99",
    "editorCursor.foreground": "#3AA99F",
    "editorWhitespace.foreground": "#4A658580",
    "editorIndentGuide.background": "#4A658533",
    "editorIndentGuide.activeBackground": "#4A658599",
    
    "panel.background": "#1D2A36",
    "panel.border": "#34495E",
    "panelTitle.activeForeground": "#F5F5F5",
    "panelTitle.inactiveForeground": "#B1BFD0",
    "editorGroupHeader.tabsBorder": "#34495E",
    
    "input.background": "#233240",
    "input.foreground": "#F5F5F5",
    "input.placeholderForeground": "#8EA2B9",
    "inputOption.activeBorder": "#008080",
    
    "list.activeSelectionBackground": "#34495E",
    "list.activeSelectionForeground": "#F5F5F5",
    "list.inactiveSelectionBackground": "#2C3E50",
    "list.inactiveSelectionForeground": "#F5F5F5",
    "list.hoverBackground": "#2C3E5066",
    "list.highlightForeground": "#3AA99F",
    
    "scrollbarSlider.background": "#4A658533",
    "scrollbarSlider.hoverBackground": "#4A658566",
    "scrollbarSlider.activeBackground": "#4A658599",
    
    "badge.background": "#008080",
    "badge.foreground": "#F5F5F5",
    "notificationCenter.border": "#008080",
    "notificationsErrorIcon.foreground": "#D14D41",
    "notificationsWarningIcon.foreground": "#DA702C",
    "notificationsInfoIcon.foreground": "#4385BE",
    
    "gitDecoration.modifiedResourceForeground": "#DA702C",
    "gitDecoration.addedResourceForeground": "#879A39",
    "gitDecoration.deletedResourceForeground": "#D14D41",
    "gitDecoration.untrackedResourceForeground": "#3AA99F",
    "gitDecoration.ignoredResourceForeground": "#6B84A2",
    "gitDecoration.conflictingResourceForeground": "#CE5D97",
    
    "breadcrumb.foreground": "#8EA2B9",
    "breadcrumb.focusForeground": "#F5F5F5",
    "breadcrumb.activeSelectionForeground": "#F5F5F5",
    "breadcrumbPicker.background": "#233240"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6B84A2"
      }
    },
    {
      "name": "String",
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "foreground": "#3AA99F"
      }
    },
    {
      "name": "Regular Expressions and Escape Characters",
      "scope": ["string.regexp", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#3AA99F"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#8B7EC8"
      }
    },
    {
      "name": "Built-in constants",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#D0A215"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.parameter", "support.variable"],
      "settings": {
        "foreground": "#4385BE"
      }
    },
    {
      "name": "Member Variables",
      "scope": ["variable.member"],
      "settings": {
        "foreground": "#D14D41"
      }
    },
    {
      "name": "Language variable",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#CE5D97"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage"],
      "settings": {
        "foreground": "#879A39"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#879A39"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#B3B6B7"
      }
    },
    {
      "name": "Separators like ; or ,",
      "scope": ["punctuation.separator", "punctuation.terminator"],
      "settings": {
        "foreground": "#B3B6B7"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation.section"],
      "settings": {
        "foreground": "#F5F5F5"
      }
    },
    {
      "name": "Accessor",
      "scope": ["punctuation.accessor"],
      "settings": {
        "foreground": "#B3B6B7"
      }
    },
    {
      "name": "Types, Classes",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.type"
      ],
      "settings": {
        "foreground": "#4385BE"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#DA702C"
      }
    },
    {
      "name": "Methods",
      "scope": ["meta.method entity.name.function"],
      "settings": {
        "foreground": "#DA702C"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#4385BE"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#D0A215"
      }
    },
    {
      "name": "CSS IDs",
      "scope": ["entity.other.attribute-name.id"],
      "settings": {
        "foreground": "#DA702C"
      }
    },
    {
      "name": "CSS Properties",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#4385BE"
      }
    },
    {
      "name": "HTML Tags",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#4385BE"
      }
    },
    {
      "name": "Markup - Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#D14D41"
      }
    },
    {
      "name": "Markup - Bold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#D0A215"
      }
    },
    {
      "name": "Markup - Heading",
      "scope": ["markup.heading"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#879A39"
      }
    },
    {
      "name": "Markup - Underline",
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#8B7EC8"
      }
    },
    {
      "name": "Markup - Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#3AA99F"
      }
    },
    {
      "name": "Markup - Quote",
      "scope": ["markup.quote"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#DA702C"
      }
    },
    {
      "name": "Markup - Raw",
      "scope": ["markup.raw"],
      "settings": {
        "foreground": "#3AA99F"
      }
    },
    {
      "name": "Markup - Fenced Code Block",
      "scope": ["markup.fenced_code.block"],
      "settings": {
        "foreground": "#F5F5F5"
      }
    },
    {
      "name": "Import",
      "scope": ["keyword.control.import"],
      "settings": {
        "foreground": "#D14D41"
      }
    },
    {
      "name": "JSX Tags",
      "scope": [
        "meta.tag.jsx entity.name.tag",
        "meta.tag.components.jsx entity.name.tag"
      ],
      "settings": {
        "foreground": "#4385BE"
      }
    },
    {
      "name": "JSX Tag Punctuation",
      "scope": [
        "meta.tag.jsx punctuation.definition.tag",
        "meta.tag.components.jsx punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#4385BE"
      }
    },
    {
      "name": "JSON Key",
      "scope": ["meta.structure.dictionary.json support.type.property-name.json"],
      "settings": {
        "foreground": "#4385BE"
      }
    },
    {
      "name": "JSON Value",
      "scope": ["meta.structure.dictionary.json meta.structure.dictionary.value.json string.quoted.double.json"],
      "settings": {
        "foreground": "#3AA99F"
      }
    }
  ]
}